    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    label{
        font-weight:bold;
    }
</style>

<h1 class="text-center">Editar a <span class="text-primary">@Html.DisplayName(Model.nombre)</span></h1>

